{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sdm-covid19-adf"
		},
		"ls_sdmcovid19stg01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sdmcovid19stg01'"
		},
		"ls_sdmcovid19stg02_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sdmcovid19stg02'"
		},
		"ls_databricks_covid19_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_covid19_cluster'"
		},
		"ls_azure_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azure_sql'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sdmcovid19sql.database.windows.net;Initial Catalog=covid19db"
		},
		"ls_http_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"ls_sdmcovid19stg02_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sdmcovid19stg02.dfs.core.windows.net/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e8f8b10c-23ed-468c-8336-b4cfd55bc1aa/resourceGroups/sdm-covid19-rg/providers/Microsoft.Storage/storageAccounts/sdmcovid19stg01"
		},
		"ls_databricks_covid19_cluster_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0307-200001-ovli8wgq"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sdm-covid19-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sdmcovid19stg01_connectionString": {
						"value": "[parameters('ls_sdmcovid19stg01_connectionString')]"
					},
					"ls_sdmcovid19stg02_accountKey": {
						"value": "[parameters('ls_sdmcovid19stg02_accountKey')]"
					},
					"ls_databricks_covid19_cluster_accessToken": {
						"value": "[parameters('ls_databricks_covid19_cluster_accessToken')]"
					},
					"ls_azure_sql_connectionString": {
						"value": "[parameters('ls_azure_sql_connectionString')]"
					},
					"ls_http_github_properties_typeProperties_url": {
						"value": "[parameters('ls_http_github_properties_typeProperties_url')]"
					},
					"ls_sdmcovid19stg02_properties_typeProperties_url": {
						"value": "[parameters('ls_sdmcovid19stg02_properties_typeProperties_url')]"
					},
					"ls_databricks_covid19_cluster_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_covid19_cluster_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sdm-covid19-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sdm-covid19-adf_ArmTemplate_0')]"
			]
		}
	]
}